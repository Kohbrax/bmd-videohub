{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./dist",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": ["es5", "es6"],
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"declaration": true,
	},
	"include": ["src"],
	"exclude": [
		"node_modules"
	]
}
